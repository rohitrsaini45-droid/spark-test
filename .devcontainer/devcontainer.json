{
  "name": "Spark Delta Dev",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspace",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "settings": {
    "python.pythonPath": "/usr/bin/python3"
  },
  "extensions": [
    "ms-python.python"
  ]
}
